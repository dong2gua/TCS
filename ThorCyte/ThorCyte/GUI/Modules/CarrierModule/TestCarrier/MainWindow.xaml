<Window x:Name="mainWind"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:regions="http://prismlibrary.com/"
        xmlns:inf="clr-namespace:ThorCyte.Infrastructure.Commom;assembly=ThorCyte.Infrastructure"
        x:Class="TestCarrier.MainWindow"
        Title="MainWindow" Height="Auto" Width="Auto">
    <Window.Resources>
    </Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ToolBar Grid.Row="0" Height="Auto">
            <Button Content="Open" Click="Open_new"/>
        </ToolBar>

        <ContentControl x:Name="MainRegion" Grid.Row="1" regions:RegionManager.RegionName="{x:Static inf:RegionNames.ReviewCarrierRegion}" />
    </Grid>
    
