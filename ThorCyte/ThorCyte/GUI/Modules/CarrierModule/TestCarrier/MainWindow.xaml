<Window x:Name="mainWind"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:regions="http://prismlibrary.com/"
        xmlns:inf="clr-namespace:ThorCyte.Infrastructure.Commom;assembly=ThorCyte.Infrastructure"
        x:Class="TestCarrier.MainWindow"
        Title="MainWindow">
    <Window.Resources>
    </Window.Resources> 
    <StackPanel>
        <ToolBar Height="Auto">
            <Button Content="Open" Click="Open_new"/>
            <Button Content="Review" Click="Review_Rgeion"/>
            <Button Content="Analysis" Click="Analysis_Region"/>
        </ToolBar>

        <ContentControl x:Name="MainRegionRev" regions:RegionManager.RegionName="{x:Static inf:RegionNames.ReviewCarrierRegion}" Width="100"/>
        
        <ContentControl x:Name="MainRegionAnl" regions:RegionManager.RegionName="{x:Static inf:RegionNames.AnalysisCarrierRegion}" Height="100"/>

    </StackPanel>
</Window>
