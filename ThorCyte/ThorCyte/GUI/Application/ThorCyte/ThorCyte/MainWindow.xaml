<Window x:Class="ThorCyte.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:regions="http://prismlibrary.com/"
        xmlns:inf="clr-namespace:ThorCyte.Infrastructure.Commom;assembly=ThorCyte.Infrastructure"
        Title="MainWindow" Height="1024" Width="1280" Style ="{StaticResource {x:Type Window}}" Icon="ThorCyte.png" MinWidth="1280" MinHeight="1024">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <ContentControl Name="TabRegion" regions:RegionManager.RegionName="{x:Static inf:RegionNames.TabRegion}" />
        <ContentControl Name="MainRegion" regions:RegionManager.RegionName="{x:Static inf:RegionNames.MainRegion}" Grid.Row="1" Margin="0,2,0,0"/>
    </Grid>
</Window>
